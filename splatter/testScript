# Create Splatts:
curl -i -H "Content-type: application/json" -X POST http://ford.sqrawler.com:3000/splatts -d '{"body":"First Splatt", "user_id":"542387e569702d7e23030000"}'
curl -i -H "Content-type: application/json" -X POST http://ford.sqrawler.com:3000/splatts -d '{"body":"Second Splatt", "user_id":"542387ef69702d7e23040000"}'
curl -i -H "Content-type: application/json" -X POST http://ford.sqrawler.com:3000/splatts -d '{"body":"Third Splatt", "user_id":"542387f669702d7e23050000"}'
curl -i -H "Content-type: application/json" -X POST http://ford.sqrawler.com:3000/splatts -d '{"body":"Fourth Splatt", "user_id":"542387ef69702d7e23040000"}'
curl -i -H "Content-type: application/json" -X POST http://ford.sqrawler.com:3000/splatts -d '{"body":"Fifth Splatt", "user_id":"542387e569702d7e23030000"}'

# Create Follows:
curl -i -H "Content-type: application/json" -X POST http://ford.sqrawler.com:3000/users/follows -d '{"id":"542387e569702d7e23030000", "follows_id":"542387ef69702d7e23040000"}'
curl -i -H "Content-type: application/json" -X POST http://ford.sqrawler.com:3000/users/follows -d '{"id":"542387e569702d7e23030000", "follows_id":"542387f669702d7e23050000"}'
